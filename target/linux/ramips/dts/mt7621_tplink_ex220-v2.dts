#include "mt7621.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/leds/common.h>

/ {
	compatible = "tplink,ex220-v2", "mediatek,mt7621-soc";
	model = "TP-Link EX220 v2";

	aliases {
		led-boot = &led_power;
		led-failsafe = &led_power;
	        led-running = &led_power;
		led-upgrade = &led_power;
	        label-mac-device = &gmac0;
	};

	chosen {
		bootargs = "console=ttyS0,115200";
	};

	keys {
		compatible = "gpio-keys";

		button-wps {
			label = "rfkill";
			gpios = <&gpio 7 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RFKILL>;
		};

		button-reset {
			label = "reset";
			gpios = <&gpio 8 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};

	leds {
		compatible = "gpio-leds";

		led-wps {
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_WPS;
			gpios = <&gpio 3 GPIO_ACTIVE_LOW>;
		};

		led-lan {
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_LAN;
			gpios = <&gpio 4 GPIO_ACTIVE_LOW>;
		};

		led-wan-orange {
			color = <LED_COLOR_ID_AMBER>;
			function = LED_FUNCTION_WAN;
			gpios = <&gpio 13 GPIO_ACTIVE_LOW>;
		};

		led_wan_green: led-wan-green {
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_WAN_ONLINE;
			gpios = <&gpio 14 GPIO_ACTIVE_LOW>;
		};

		led-wifi5g {
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_WLAN;
			function-enumerator = <5>;
			gpios = <&gpio 15 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "phy1tpt";
		};

		led-wifi2g {
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_WLAN;
			function-enumerator = <2>;
			gpios = <&gpio 16 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "phy0tpt";
		};

		led_power: led-power {
			color = <LED_COLOR_ID_GREEN>;
			function = LED_FUNCTION_POWER;
			gpios = <&gpio 18 GPIO_ACTIVE_LOW>;
		};
	};
};

&nand {
	status = "okay";
	mediatek,nmbm;

    partitions {
        compatible = "fixed-partitions";
        #address-cells = <1>;
        #size-cells = <1>;
	nand-ecc-mode = "hw";
        partition@0 {
            label = "u-boot";
            reg = <0x00000000 0x00080000>;
            read-only;
        };

        partition@80000 {
            label = "u-boot-env";
            reg = <0x00080000 0x00080000>;
        };

        partition@100000 {
            label = "misc_ro";
            reg = <0x00100000 0x00600000>;
            read-only;

	    nvmem-layout {
	        compatible = "fixed-layout";
	        #address-cells = <1>;
		#size-cells = <1>;

		macaddr_1: macaddr@5c30bc {
			compatible = "mac-base";
			reg = <0x5c30bc 0x6>;
			#nvmem-cell-cells = <1>;
		};
	    };
        };

        partition@700000 {
            label = "misc_rw";
            reg = <0x00700000 0x00600000>;
        };

	partition@d00000 {
	    label = "firmware";
	    reg = <0x00d00000 0x02c00000>;
	
	    compatible = "fixed-partitions";
	    #address-cells = <1>;
	    #size-cells = <1>;
	
	    partition@0 {
	        label = "kernel";
	        reg = <0x0 0x360000>;
	    };
	
	    partition@400000 {
	        label = "ubi";
	        reg = <0x360000 0x2700000>;
	    };
	};

        partition@3900000 {
            label = "bflag";
            reg = <0x03900000 0x00600000>;
            read-only;
        };

        partition@3f00000 {
            label = "misc_rw_bak";
            reg = <0x03f00000 0x00600000>;
        };

        partition@4500000 {
            label = "misc_isp";
            reg = <0x04500000 0x00600000>;
            read-only;
        };

        partition@0whole {
            label = "whole";
            reg = <0x00000000 0x08000000>;
        };
    };
};

&pcie {
	status = "okay";
};

&pcie1 {
	wifi0: wifi@0,0 {
		compatible = "mediatek,mt76";
		reg = <0x0000 0 0 0 0>;
		nvmem-cells = <&macaddr_1 0>;
		nvmem-cell-names = "mac-address";
		mediatek,disable-radar-background;
	};
};

&ethernet {
	pinctrl-0 = <&mdio_pins>, <&rgmii1_pins>, <&rgmii2_pins>;
};

&gmac0 {
	phy-mode = "trgmii";
	nvmem-cells = <&macaddr_1 0>;
	nvmem-cell-names = "mac-address";
};

&gmac1 {
        status = "okay";
	label = "wan";
        phy-handle = <&ethphy0>;

	nvmem-cells = <&macaddr_1 0>;
	nvmem-cell-names = "mac-address";
};

&switch0 {
	ports {
		port@0 {
			status = "okay";
			label = "lan1";
		};

		port@1 {
			status = "okay";
			label = "lan2";
		};

		port@2 {
			status = "okay";
			label = "lan3";
		};

		port@3 {
			status = "okay";
			label = "lan4";
		};

		port@4 {
			status = "okay";
			label = "wan";
		};

		port@6 {
			status = "okay";
			label = "lan6";
		};
	};
};

&state_default {
	gpio {
		groups = "i2c", "uart3", "jtag", "wdt";
		function = "gpio";
	};
};

&gdma {
	status = "okay";
};

&hsdma {
	status = "okay";
};
